<style>

    .lop-ban-quyen {
        clear: both;
    }

    .lop-ban-q-trai {
        float: left;
        width: 40%;
        text-align: center;
    }

    .lop-ban-q-phai {
        float: right;
        width: 60%;
        text-align: right;
    }
    .login{
        color:white;
        text-align:right;
        padding-top:0px;
        text-decoration: none;
        
    }
     .container-1 {
        width: 160px;
        vertical-align: middle;
        white-space: nowrap;
        position: relative;
        margin-left: 10px;
    }

        .container-1 input#search {
            width: 30px;
            margin-top: 0px;
            height: 30px;
            background: #2b303b;
            border: none;
            font-size: 10pt;
            float: left;
            color: #262626;
            padding-left: 35px;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            boder-radius: 5px;
            color: #fff;
            /*Hiệu ứng chuyển tiếp*/
            -webkit-transition: width .55s ease;
            -moz-transition: width.55s ease;
            -o-transition: width .55s ease;
            transition: width .55s ease;
            margin-bottom:10px;
        }

            .container-1 input#search:-webkit-input-placeholder {
                color: #65737e;
            }

            .container-1 input#search:-moz-placeholder { /*Firefox 18- */
                color: #65737e;
            }

            .container-1 input#search:-moz-placeholder { /*Firefox 19+ */
                color: #65737e;
            }

            .container-1 input#search:-ms-input-placeholder {
                color: #65737e;
            }
        /*Biểu tượng*/
        .container-1 .icon {
            position: absolute;
            top: 10%;
            margin-left: 12px;
            margin-top: 5px;
            z-index: 1;
            color: #fff;
            margin-bottom:7px;
        }
        /* Hiệu ứng Hover */
        .container-1 input#search:focus, .container-1 input#search:active {
            outline: none;
            width: 160px;
            color: #65737e;
        }

        .container-1:hover input#search {
            width: 160px;
            color: #65737e;
        }

        .container-1:hover .icon {
            color: #93a2ad;
        }
